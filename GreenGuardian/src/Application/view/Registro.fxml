<AnchorPane fx:id="AP" prefHeight="600.0" prefWidth="800.0" styleClass="windows" stylesheets="@Ventana.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="94.0" fitWidth="229.0" layoutX="300.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
