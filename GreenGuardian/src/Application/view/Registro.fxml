      </TextField>
      <TextField fx:id="txtRepetirContr" layoutX="520.0" layoutY="384.0" prefHeight="33.0" prefWidth="211.0" promptText="Repita su contraseña" styleClass="bordes" stylesheets="@Bordes.css">
