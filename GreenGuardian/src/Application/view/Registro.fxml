            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="btnVolverR" layoutX="193.0" layoutY="470.0" mnemonicParsing="false" onAction="#cerrarRegsitro" prefHeight="33.0" prefWidth="107.0" text="Volver" textFill="WHITE">
