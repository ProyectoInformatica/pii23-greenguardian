      </ImageView>
      <TextField fx:id="txtIntroNombre" layoutX="98.0" layoutY="233.0" prefHeight="33.0" prefWidth="211.0" promptText="Introduzca su nombre" styleClass="bordes" stylesheets="@Bordes.css">
