<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="windows" stylesheets="@Ventana.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="94.0" fitWidth="229.0" layoutX="300.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Img/LogoGG.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtUsuario" layoutX="309.0" layoutY="243.0" prefHeight="33.0" prefWidth="211.0" promptText="Ingrese su usuario" styleClass="bordes" stylesheets="@Bordes.css">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="txtConstra" layoutX="309.0" layoutY="325.0" prefHeight="33.0" prefWidth="211.0" promptText="Ingrese su contraseña" styleClass="bordes" stylesheets="@Bordes.css">
