            <URL value="@Bordes.css" />
         </stylesheets></Button>
      <Button fx:id="btnRegistrar" layoutX="529.0" layoutY="437.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="107.0" text="Registrarme" textFill="WHITE">
         <stylesheets>
            <URL value="@Botones.css" />
            <URL value="@Bordes.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="botones" />
            <String fx:value="bordes" />
         </styleClass></Button>
   </children>
</AnchorPane>
